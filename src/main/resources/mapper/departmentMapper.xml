<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 부서 sql -->
<mapper namespace="com.bulemine.www.interfaces/departmentMapper">
<!-- 부서추가 -->
<insert id="" parameterType="cpDepartment">
INSERT INTO CP_DEPARTMENT
(
    DEPNameEN
    ,DEPNAMEKR
    ,typeAlias
)VALUES(
    #{depNameEn}
    ,#{depNameKr}
    ,#{typeAlias}
);
</insert>
  
  
<!-- 부서 조회 -->
<select id="" resultType="cpDepartment">
SELECT 
	depNameEn
	,depNameKr
	, typeAlias
FROM CP_DEPARTMENT
</select>  
</mapper>